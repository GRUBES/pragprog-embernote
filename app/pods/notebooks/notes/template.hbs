<div class="col-md-12">
  Notes
</div>
<div>
  <div class="col-md-4">
    <section>
      title: {{input value=title}} <button {{action 'addNote'}}>Add</button>
    </section>
    <section>
      <ul>
      {{#each model as |note|}}
        <li>
        {{#link-to 'notebooks.notes.note' note.id}}
          {{note.title}}
        {{/link-to}}
        <button {{action 'deleteNote' note}}>Delete</button>
        </li>
      {{/each}}
      </ul>
    </section>
  </div>
  <div class="col-md-8">{{outlet}}</div>
</div>
